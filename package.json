{
  "name": "7vswild",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "npm:install": "npm install",
    "--- dev": "this is a divider",
    "dev": "npm run build:html -- --dev && rollup -c -w",
    "start-by-rollup": "sirv public --no-clear",
    "validate": "svelte-check",
    "--- deploy": "this is a divider",
    "build": "rollup -c && npm run build:html",
    "build:html": "npm run exec ./scripts/build.js",
    "exec": "node --experimental-json-modules --no-warnings"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "fs-extra": "^10.0.0",
    "modularscale-sass": "^3.0.10",
    "node-sass": "^6.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-svelte-svg": "^1.0.0-beta.2",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-apollo": "^0.4.0",
    "svelte-assets-preprocessor": "^0.0.7",
    "svelte-check": "^1.0.0",
    "svelte-click-outside": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "svelte-watch-resize": "^1.0.3",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "sirv-cli": "^1.0.0"
  }
}
